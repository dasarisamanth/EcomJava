<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Ecom</display-name>
  <servlet>
  <servlet-name>SignUp</servlet-name>
  <servlet-class>com.ecom.SignUp</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>SignUp</servlet-name>
  <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>Logout</servlet-name>
  <servlet-class>com.ecom.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Logout</servlet-name>
  <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>Success</servlet-name>
  <jsp-file>loginsuccess.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  <servlet-name>Success</servlet-name>
  <url-pattern>/success</url-pattern>
  </servlet-mapping>
  <filter>
  <filter-name>Authentication</filter-name>
  <filter-class>com.ecom.AuthenticationFilter</filter-class>
  </filter>
  <filter-mapping>
  <filter-name>Authentication</filter-name>
  <url-pattern>/success</url-pattern>
  </filter-mapping>
</web-app>